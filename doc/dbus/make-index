#!/bin/sh
echo "<!-- do not edit: generated by make-index -->"
echo "<head><title>Agama D-Bus API</title></head>"
echo "<ul>"
for F in tmp/ref-*.xml; do
  F=${F#tmp/}
  F=${F%.xml}
  echo "<li><a href=\"${F}.html\""
  echo "                >${F#ref-}</a></li>"
done
echo "</ul>"
echo "<a href="..">up</a>"
